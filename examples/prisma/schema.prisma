datasource postgresql {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}


model List1 {
  id   String @id
  name String @default("")
  User User[]
}

model List2 {
  id   String @id
  name String @default("")
  User User[] @relation("User_list2s")
}

model List3 {
  id   String  @id
  name String  @default("")
  user BigInt? @unique
  User User?   @relation(fields: [user], references: [id])
}

model List4 {
  id   String  @id
  name String  @default("")
  user BigInt?
  User User?   @relation(fields: [user], references: [id])

  @@index([user])
}

model List5 {
  id   String @id
  name String @default("")
  User User[] @relation("List5_users")
}

model List6 {
  id               String @id
  name             String @default("")
  User_List6_users User[] @relation("List6_users")
  User_User_list6s User[] @relation("User_list6s")
}

model List7 {
  id                     String  @id
  name                   String  @default("")
  user                   BigInt?
  User_List7_userToUser  User?   @relation("List7_userToUser", fields: [user], references: [id])
  User_List7ToUser_list7 User[]  @relation("List7ToUser_list7")

  @@index([user])
}

model List8 {
  id               String  @id
  name             String  @default("")
  user             BigInt?
  User_List8ToUser User?   @relation(fields: [user], references: [id])
  User_User_list8  User[]  @relation("User_list8")

  @@index([user])
}

model PhoneNumber {
  id                     BigInt  @id @default(autoincrement())
  user                   BigInt?
  type                   String?
  value                  String  @default("")
  User_PhoneNumberToUser User?   @relation(fields: [user], references: [id])
  User_User_phoneNumbers User[]  @relation("User_phoneNumbers")

  @@index([user])
}

model Post {
  id          BigInt    @id @default(autoincrement())
  title       String    @default("")
  status      String    @default("draft")
  content     Json      @default("[{\"type\": \"paragraph\", \"children\": [{\"text\": \"\"}]}]")
  publishDate DateTime?
  author      BigInt?
  User        User?     @relation(fields: [author], references: [id])

  @@index([author])
}


model User {
  id                            BigInt        @id @default(autoincrement())
  name                          String        @default("")
  email                         String        @unique @default("")
  avatar_filesize               Int?
  avatar_extension              String?
  avatar_width                  Int?
  avatar_height                 Int?
  avatar_id                     String?
  attachment_filesize           Int?
  attachment_filename           String?
  password                      String?
  isAdmin                       Boolean       @default(false)
  roles                         String        @default("")
  list1                         String?
  list7                         String?
  List1                         List1?        @relation(fields: [list1], references: [id])
  List7_List7ToUser_list7       List7?        @relation("List7ToUser_list7", fields: [list7], references: [id])
  List3                         List3?
  List4                         List4[]
  List7_List7_userToUser        List7[]       @relation("List7_userToUser")
  List8_List8ToUser             List8[]
  PhoneNumber_PhoneNumberToUser PhoneNumber[]
  Post                          Post[]
  List5                         List5[]       @relation("List5_users")
  List6_List6_users             List6[]       @relation("List6_users")
  List2                         List2[]       @relation("User_list2s")
  List6_User_list6s             List6[]       @relation("User_list6s")
  List8_User_list8              List8[]       @relation("User_list8")
  PhoneNumber_User_phoneNumbers PhoneNumber[] @relation("User_phoneNumbers")

  @@index([list1])
  @@index([list7])
}
